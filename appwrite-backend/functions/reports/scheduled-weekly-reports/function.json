{
  "runtime": "node-18.0",
  "entrypoint": "src/main.js",
  "commands": "npm install && npm start",
  "schedule": "0 9 * * 1",
  "timeout": 300,
  "enabled": true,
  "logging": true,
  "loggingConfig": {
    "level": "info",
    "maxSize": 10485760,
    "maxFiles": 5
  },
  "variables": [
    {
      "key": "APPWRITE_ENDPOINT",
      "value": "https://cloud.appwrite.io/v1"
    },
    {
      "key": "APPWRITE_PROJECT_ID",
      "value": ""
    },
    {
      "key": "APPWRITE_API_KEY",
      "value": ""
    },
    {
      "key": "APPWRITE_DATABASE_ID",
      "value": "default"
    },
    {
      "key": "APPWRITE_STORAGE_BUCKET_ID",
      "value": "reports"
    },
    {
      "key": "SMTP_HOST",
      "value": "smtp.gmail.com"
    },
    {
      "key": "SMTP_PORT",
      "value": "587"
    },
    {
      "key": "SMTP_USER",
      "value": ""
    },
    {
      "key": "SMTP_PASS",
      "value": ""
    },
    {
      "key": "EMAIL_FROM",
      "value": "noreply@immuneme.com"
    },
    {
      "key": "REPORT_SCHEDULE",
      "value": "0 9 * * 1"
    },
    {
      "key": "REPORT_RECIPIENTS",
      "value": ""
    },
    {
      "key": "REPORT_FORMATS",
      "value": "pdf,excel,csv"
    },
    {
      "key": "REPORT_RETENTION_DAYS",
      "value": "90"
    },
    {
      "key": "SCHEDULE_TYPE",
      "value": "weekly"
    }
  ]
}